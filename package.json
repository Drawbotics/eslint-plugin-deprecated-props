{
  "name": "@drawbotics/eslint-plugin-deprecated-props",
  "version": "0.1.0",
  "description": "Small eslint plugin to report usage of deprecated props",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --build",
    "test:integration": "cd tests/integration && npm run test",
    "test": "npm run build && npm run test:integration",
    "lint:integration": "npm run build && cd tests/integration && npm run lint"
  },
  "author": "nicmosc <nmoscholios@gmail.com>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": "eslint --format node_modules/eslint-formatter-pretty --max-warnings=0"
  },
  "dependencies": {
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@drawbotics/react-drylus": "^6.2.1-beta.45",
    "@types/eslint": "^6.8.0",
    "@types/estree": "0.0.44",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/experimental-utils": "^2.29.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "import-sort-style-module": "^6.0.0",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "prettier-plugin-import-sort": "0.0.4",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0"
  }
}
